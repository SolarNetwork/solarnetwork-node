<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%20.20t] %-5level %35.35c - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="net.solarnetwork" level="trace"/>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>