<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
  "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
  "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<definition name="plugins" extends="default">
		<put-attribute name="navloc" value="plugins" type="string"/>
		<put-attribute name="head" value="/WEB-INF/jsp/a/plugins/plugins-head.jsp"/>
		<put-attribute name="header" value="/WEB-INF/jsp/header.jsp"/>
	</definition>
	
	<definition name="plugins/*" extends="plugins">
		<put-attribute name="body" value="/WEB-INF/jsp/a/plugins/{1}.jsp" />
	</definition>
		
</tiles-definitions>
