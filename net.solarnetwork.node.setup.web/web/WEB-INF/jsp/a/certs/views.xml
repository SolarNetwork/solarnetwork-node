<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
  "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
  "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<definition name="certs" extends="default">
		<put-attribute name="navloc" value="cert" type="string"/>
		<put-attribute name="head" value="/WEB-INF/jsp/a/certs/certs-head.jsp"/>
	</definition>
	
	<definition name="certs/*" extends="certs">
		<put-attribute name="body" value="/WEB-INF/jsp/a/certs/{1}.jsp" />
	</definition>
		
</tiles-definitions>
