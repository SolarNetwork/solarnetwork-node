<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>

	<definition name="certs" extends="default">
		<put-attribute name="navloc" value="cert" type="string"/>
		<put-attribute name="head" value="/WEB-INF/jsp/a/cert/cert-head.jsp"/>
	</definition>
	
	<definition name="certs/*" extends="settings">
		<put-attribute name="body" value="/WEB-INF/jsp/a/certs/{1}.jsp" />
	</definition>
		
</tiles-definitions>
